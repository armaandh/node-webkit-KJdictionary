		.tabbing
			label
				input#tab-1.display-none(type='radio', name='menu-tab', value='tab-1', data_tab_number='1', checked='')
				#daum.tab Daum
			label
				input#tab-2.display-none(type='radio', name='menu-tab', value='tab-2', data_tab_number='2')
				#weblio.tab Weblio
			ol
				li.tab-item
					.tab-background
						.tab-text
							.title
								table#miyazaki
									thead
										tr
											th English
											th Korean
										tbody
											each i in kor_e_arr
												tr
													td=i


